! Noel's RetroLab Benchmark

BEGIN
    VAR START = MILLIS()
    VAR ST = SECONDS()
    VAR S
    FOR i=1 TO 10
        s=0
        FOR j=1 TO 1000
            s=s+j
        NEXT j
        PRINT ".";
    NEXT i
    PRINT s
    PRINT MILLIS() - START; " ms"
    PRINT SECONDS() - ST; " seconds"
END

